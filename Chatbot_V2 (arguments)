#include <iostream>
using namespace std;

void bot(int m, string n) {

    if (m==1){
        cout << "Welcome, " << n << "!";
    }

    else if (m==2){
        cout << "Goodbye, " << n << "!"; 
    }

    else{
        cout << "Try again";
    }

}


int main() {
    int mode;
    cin >> mode;

    string name;
    cin >> name;

    bot(mode, name);
}
