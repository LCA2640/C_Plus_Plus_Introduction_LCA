#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n; //size of the array
    //your code goes here

    int *nums = new int [n];

    for (int i=0; i<n; i++ ){
        int v;
        cin >>v;
        nums[i]= v;

        v=0;
    }

    int max = nums[0];

    for(int i=0; i<n; i++) {
        if(nums[i]>max)
            max = nums[i];
    }
    cout << max;

}
